{
  "name": "byteinspire-cloud-project-deployer-action",
  "version": "1.0.0",
  "main": "dist/index.js",
  "description": "A GitHub action to deploy ByteInspire cloud project",
  "engines": {
    "node": ">= 12 <15"
  },
  "devDependencies": {
    "@commitlint/cli": "^13.1.0",
    "@commitlint/config-conventional": "^13.1.0",
    "@types/jest": "~26.0.23",
    "@types/node": "^16.9.1",
    "@typescript-eslint/eslint-plugin": "~4.28.2",
    "@typescript-eslint/parser": "~4.28.2",
    "@vercel/ncc": "^0.31.1",
    "eslint": "~7.30.0",
    "eslint-config-prettier": "~8.3.0",
    "eslint-plugin-jest": "~24.3.6",
    "husky": "^1.2.0",
    "jest": "~27.0.6",
    "lint-staged": "^11.1.2",
    "nock": "^13.1.3",
    "prettier": "~2.3.2",
    "rimraf": "~3.0.2",
    "ts-jest": "~27.0.3",
    "tsutils": "~3.21.0",
    "typescript": "~4.3.5"
  },
  "scripts": {
    "start": "node dist/index.js",
    "clean": "rimraf coverage build tmp",
    "prebuild": "yarn lint",
    "build": "ncc build src/index.ts -o dist",
    "build:watch": "ncc build -w src/index.ts -o dist",
    "lint": "eslint . --ext .ts,.tsx",
    "test": "jest --coverage",
    "test:watch": "jest --watch"
  },
  "husky": {
    "hooks": {
      "commit-msg": "commitlint -E HUSKY_GIT_PARAMS",
      "pre-commit": "yarn build && git add dist && lint-staged",
      "pre-push": "yarn test"
    }
  },
  "lint-staged": {
    "*.ts": [
      "eslint --fix",
      "git add"
    ]
  },
  "author": "heliu <heliu.fe@bytedance.com>",
  "license": "MIT",
  "dependencies": {
    "@actions/core": "^1.9.1",
    "@actions/github": "^5.0.0",
    "archiver": "^5.3.0",
    "axios": "^0.21.4",
    "form-data": "^4.0.0",
    "tslib": "~2.3.0",
    "uuid": "^8.3.2"
  }
}
